--player update

function player_update()

	player.dy+=gravity
	player.dx*=friction
	

--controls
	if btn(0) then 
	player.dx-=player.acc
	player.flp=true
	player.moving=true
	player.shooting=false
	end

	if btn(1) then
	player.dx+=player.acc
	player.flp=false
	player.moving=true
	player.shooting=false
	end

--jump
	if btnp(❎)
	and	player.landed then
	player.dy-=player.boost
	player.landed=false
	player.shooting=false
	end
	
--shooting
	if btnp(4) then
			local newbul={x=player.x+10,y=player.y}
			add(buls,newbul)
			player.shooting=true
	end

--check collision up and down
	if player.dy>0 then
			player.falling=true
			player.landed=false
			player.jumping=false
			
			player.dy=mid(-player.max_dy,player.dy,player.max_dy)
			
			if collide_map(player,"down",0) then
					player.landed=true
					player.falling=false
					player.dy=0
					player.y-=((player.y+player.h+1)%8)-1
			end

	elseif	player.dy<0 then
			player.jumping=true
			if collide_map(player,"up",1) then
				player.dy=0
			end		
	end

--check collision left and right
	if player.dx<0 then
			
				player.dx=mid(-player.max_dx,player.dx,player.max_dx)	
		
			if collide_map(player,"left",0) then
						player.dx=0
			end
			
	elseif player.dx>0 then

		player.dx=mid(player.dx,player.max_dx)
		
			if collide_map(player,"right",0) then
					player.dx=0
			end
	end
	
	player.x+=player.dx
	player.y+=player.dy

---limiting the sprite to the map
	
	if player.x<map_start then
				player.x=map_start
	end
	if player.x>map_end-player.w then
				player.x=map_end-player.w
	end
	
--player winning
if player.x>950 then
				scene="win"
		end
	
end


