--explosion

function create_exp(_x,_y)
	sfx(1)
	add(explosion,
	{x=_x,y=_y,t=0})
	
end

function update_exp()
 for exp in all(explosion) do
 	exp.t+=1
 	if exp.t ==13 then
 		del(explosion,exp)
 	end
 end
end

function draw_exp()
		
		for exp in all (explosion) do
			circ(exp.x,exp.y,exp.t/4,
			7+exp.t%4)
		end
end	
