--enemies

function show_enemy(amount)
	gap=(1024-amount)/(amount+1)
	for i=1,amount do
		add(enemies,{sp=11,x=gap*i +8*(i-1),y=100,flp=false,dx=0.5})
		add(enemies,{sp=66,x=gap*i +8*(i-1),y=100,flp=false,dx=0.3,dy=0.5})
--		add(enemies,{x=rnd(1024),y=100,flp=false,dx=0.5})
	end
end

function update_enemy()
--move enemy
	for e in all (enemies) do
		if player.x<e.x then 
					e.x-=e.dx
					e.flp=false
		end
		if player.x>e.x then
					e.x+=e.dx
					e.flp=true
		end
		
		if e.sp==66 then
					e.y-=e.dy
		end
	end
---collision player x enemy
	for e in all (enemies) do
		if col(player,e) then
				del(enemies,e)
				lives-=1
					if lives==0 then
						scene="over"
					end
		end
	end
	
--collision enemy x bullets
	for e in all (enemies) do
		for mybul in all (buls) do
			if col(e,mybul) then
					create_exp(mybul.x,mybul.y)
					del(enemies,e)
					del(buls,mybul)
			end
		end
	end
	
end

	


