-- draw_game
function draw_game()
	cls(0)
	map()
	spr(player.sp,player.x,player.y,2,2,player.flp)
	--print(player.x,cam_x,30)
	--time
	print("time:"..flr(time()),cam_x,9)
		if (time()) >30 then
				print("time is over!",player.x+30,60,7)
				stop() 
		end
		
	--drawing lives
	for i=1,4 do
	
		if lives>=i then
			spr(96,cam_x+9*i-8,1)
		else
			spr(97,cam_x+9*i-8,1)
		end
	end

		
	--drawing enemies
	for e in all(enemies) do
	spr(e.sp,e.x,e.y,2,2)
	end
	
	--draw explosions
	draw_exp()
	
	--drawing weapons
	for i=1,#buls do
			local mybul=buls[i]
		spr(61,mybul.x,mybul.y)	
	end
end

