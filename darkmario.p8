function _draw()

	if scene=="game" then
		draw_game()
		
	elseif scene=="start" then
		draw_menu()
	elseif scene=="over" then
		draw_over()
	elseif scene=="win" then
		draw_win()
	end
	
end

function _update()

	if scene=="game" then
		update_game()
		
	elseif scene=="start" then
		update_menu()
	elseif scene=="over" then
		update_over()
	elseif scene=="win" then
		update_win()
	end

end

